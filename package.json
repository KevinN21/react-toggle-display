{
  "name": "react-toggle-display",
  "version": "1.1.0",
  "description": "Hide/show a component's children",
  "repository": {
    "type": "git",
    "url": "https://github.com/ccnokes/react-toggle-display"
  },
  "main": "./dist/index.js",
  "scripts": {
